{
  "plugins": [],
  "recurseDepth": 20,
  "source": {
      "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": false
  },
  "opts": {
      "destination": "./docs/",
      "template": "node_modules/tui-jsdoc-template"
  }
}